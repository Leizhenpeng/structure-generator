{
  "name": "structure-generator",
  "displayName": "Structure Generator",
  "description": "Powered by AI, Structure Generator optimizes project setup by swiftly creating files and directories from structure diagrams",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.88.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "menus": {
      "view/title": [
        {
          "command": "fileStructureGenerator.createFiles",
          "group": "navigation"
        },
        {
          "command": "fileStructureGenerator.createFiles",
          "group": "inline"
        }
      ]
    },
    "commands": [
      {
        "command": "fileStructureGenerator.createFiles",
        "title": "Structure Generator",
        "icon": "media/icon.svg"
      },
      {
        "command": "structure-generator.helloWorld",
        "title": "Hello 2"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.88.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "eslint": "^8.57.0",
    "typescript": "^5.3.3",
    "@vscode/test-cli": "^0.0.8",
    "@vscode/test-electron": "^2.3.9"
  }
}